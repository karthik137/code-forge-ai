{
    "name": "CodeGenie",
    "version": "1.2.7",
    "description": "",
    "main": "./src/extension.js",
	  "license": "gpl-3.0",
    "engines": {
        "vscode": "^1.1.37"
    },
    "icon": "images/CodeGenieTextFrame.png",
    "galleryBanner": {
      "color": "#EDEDFF"
    },
    "repository" : "https://github.com/karthik137/codegenie",
    "contributes": {
        "commands": [
            {
                "command": "extension.CodeGenie",
